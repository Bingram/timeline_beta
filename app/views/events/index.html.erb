<h1>Event Listing</h1>

<%= link_to 'New event', new_event_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Time</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= event.description %></td>
      <td><%= event.time_stamp %></td>
      <td><%= link_to 'Show', events_path(event) %></td>
    </tr>
  <% end %>
</table>
